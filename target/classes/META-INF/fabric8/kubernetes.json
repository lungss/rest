{
  "apiVersion" : "v1",
  "kind" : "List",
  "items" : [ {
    "apiVersion" : "v1",
    "kind" : "Service",
    "metadata" : {
      "annotations" : {
        "discovery.3scale.net/path" : "/camel",
        "discovery.3scale.net/discovery-version" : "v1",
        "discovery.3scale.net/description-path" : "/api-docs",
        "discovery.3scale.net/scheme" : "http",
        "discovery.3scale.net/port" : "8080",
        "fabric8.io/iconUrl" : "img/icons/camel.svg",
        "prometheus.io/port" : "9779",
        "prometheus.io/scrape" : "true"
      },
      "labels" : {
        "expose" : "true",
        "discovery.3scale.net" : "true",
        "app" : "rest",
        "provider" : "fabric8",
        "version" : "1.0.0",
        "group" : "org.demo"
      },
      "name" : "rest"
    },
    "spec" : {
      "ports" : [ {
        "name" : "http",
        "port" : 8080,
        "protocol" : "TCP",
        "targetPort" : 8080
      } ],
      "selector" : {
        "app" : "rest",
        "provider" : "fabric8",
        "group" : "org.demo"
      }
    }
  }, {
    "apiVersion" : "extensions/v1beta1",
    "kind" : "Deployment",
    "metadata" : {
      "annotations" : {
        "fabric8.io/iconUrl" : "img/icons/camel.svg",
        "fabric8.io/metrics-path" : "dashboard/file/camel-routes.json/?var-project=rest&var-version=1.0.0"
      },
      "labels" : {
        "app" : "rest",
        "provider" : "fabric8",
        "version" : "1.0.0",
        "group" : "org.demo"
      },
      "name" : "rest"
    },
    "spec" : {
      "replicas" : 1,
      "revisionHistoryLimit" : 2,
      "selector" : {
        "matchLabels" : {
          "app" : "rest",
          "provider" : "fabric8",
          "group" : "org.demo"
        }
      },
      "template" : {
        "metadata" : {
          "annotations" : {
            "fabric8.io/iconUrl" : "img/icons/camel.svg",
            "fabric8.io/metrics-path" : "dashboard/file/camel-routes.json/?var-project=rest&var-version=1.0.0"
          },
          "labels" : {
            "app" : "rest",
            "provider" : "fabric8",
            "version" : "1.0.0",
            "group" : "org.demo"
          }
        },
        "spec" : {
          "containers" : [ {
            "env" : [ {
              "name" : "SPRING_APPLICATION_JSON",
              "value" : "{\"server\":{\"tomcat\":{\"max-threads\":1}}}"
            }, {
              "name" : "KUBERNETES_NAMESPACE",
              "valueFrom" : {
                "fieldRef" : {
                  "fieldPath" : "metadata.namespace"
                }
              }
            } ],
            "image" : "demo/rest:1.0.0",
            "imagePullPolicy" : "IfNotPresent",
            "livenessProbe" : {
              "httpGet" : {
                "path" : "/health",
                "port" : 8081,
                "scheme" : "HTTP"
              },
              "initialDelaySeconds" : 180
            },
            "name" : "spring-boot",
            "ports" : [ {
              "containerPort" : 8080,
              "name" : "http",
              "protocol" : "TCP"
            }, {
              "containerPort" : 9779,
              "name" : "prometheus",
              "protocol" : "TCP"
            }, {
              "containerPort" : 8778,
              "name" : "jolokia",
              "protocol" : "TCP"
            } ],
            "readinessProbe" : {
              "httpGet" : {
                "path" : "/health",
                "port" : 8081,
                "scheme" : "HTTP"
              },
              "initialDelaySeconds" : 10
            },
            "resources" : {
              "limits" : {
                "cpu" : "1.0"
              },
              "requests" : {
                "cpu" : "0.2"
              }
            },
            "securityContext" : {
              "privileged" : false
            }
          } ]
        }
      }
    }
  } ]
}